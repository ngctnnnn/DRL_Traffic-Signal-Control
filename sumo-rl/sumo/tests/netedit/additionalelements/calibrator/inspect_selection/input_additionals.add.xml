<additional xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/additional_file.xsd">
	<routeProbe id="RouteProbeA" edge="gneE0" freq="3600" file="RouteProbeA.xml" begin="0.00"/>
	<routeProbe id="RouteProbeB" edge="gneE0" freq="3600" file="RouteProbeB.xml" begin="0.00"/>
	<routeProbe id="customRouteProbeName" edge="gneE0" freq="3600" file="RouteProbeB.xml" begin="0.00"/>
	
	<!-- calibrator A -->
    <calibrator id="calibratorA" edge="gneE2" pos="20" freq="30" name="NameA" routeProbe="RouteProbeA" output="fileA.txt">
		<param key="KeyA1" value="valueA1"/>
		<param key="KeyA3" value="valueA2"/>
		<param key="KeyA2" value="valueA3"/>
	</calibrator>
	
	<!-- calibrator B -->
	<calibrator id="calibratorB" edge="gneE3" pos="30" freq="40" name="NameB" routeProbe="RouteProbeB" output="fileB.txt">
		<param key="KeyB1" value="valueB1"/>
		<param key="KeyB3" value="valueB2"/>
		<param key="KeyB2" value="valueB3"/>
	</calibrator>
</additional>
