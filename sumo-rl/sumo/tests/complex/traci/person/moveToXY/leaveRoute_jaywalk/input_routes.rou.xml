<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <vType id="DEFAULT_VEHTYPE">
        <param key="has.vehroute.device" value="false"/>
        <param key="has.fcd.device" value="false"/>
    </vType>

    <person id="p0" depart="0" departPos="40">
        <walk edges="SC NC"/>
    </person>
    <flow id="SN" begin="0" end="50" from="SC" to="CN" departSpeed="max" period="2" departLane="random"/>
    <flow id="NS" begin="0" end="50" from="NC" to="CS" departSpeed="max" period="2" departLane="random"/>
</routes>
