<configuration>

    <input>
        <net-file value="net.net.xml"/>
        <route-files value="input_routes.rou.xml"/>
    </input>

    <time>
        <step-length value="0.25" />
    </time>
    
    <processing>
        <lateral-resolution value="0.64"/>
        <step-method.ballistic value="true"/>
