# no jam detection (causing suboptimal timing)
default

# detect blockage after 5s of jamming
jamThresh_5

# detect blockage after 5s of jamming (set via option)
jamThresh_5_option

# set for a specific lane rather than for all detectors
jamThresh_5_lane

# detect blockage after 10s of jamming
jamThresh_10

# detect blockage after 5s of jamming
useAllDetectors
