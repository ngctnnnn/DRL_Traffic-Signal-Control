<additional xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/additional_file.xsd">
	<rerouter id="rerouter" edges="beg2left">
		<interval begin="50" end="1000">
			<closingReroute id="left" disallow="all"/>
			<destProbReroute id="keepDestination" probability="0.33"/>
			<destProbReroute id="terminateRoute" probability="0.33"/>
			<destProbReroute id="right" probability="0.33"/>
		</interval>
	</rerouter>
</additional>
